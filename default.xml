                  <copyfile dest="README" src="README"/>
                  <copyfile dest="setup-environment" src="setup-environment"/>
  </project>
  <project remote="freescale"   revision="dizzy"    name="meta-fsl-arm-extra"       path="sources/meta-fsl-arm-extra"/>
