  <project remote="freescale"   revision="dizzy"    name="Documentation"            path="sources/Documentation"/>
  <project remote="raspberrypi" revision="daisy"    name="meta-raspberrypi"         path="sources/meta-raspberrypi"/>
  <project remote="xilinx"      revision="dizzy"    name="meta-xilinx"              path="sources/meta-xilinx"/>
  <project remote="parallella"  revision="master"   name="meta-parallella"          path="sources/meta-parallella"/>
  <project remote="altera"      revision="master"   name="meta-altera"              path="sources/meta-altera"/>
